{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "endtoenddatafactory53"
		},
		"AzureDataLakeStorageLS_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorageLS'"
		},
		"AzureSqlDatabase_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabase'"
		},
		"AzureSqldbls_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqldbls'"
		},
		"endtoendls1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'endtoendls1'"
		},
		"AzureDataLakeStorageLS_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://endtoendstorage1.dfs.core.windows.net/"
		},
		"endtoendls1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://endtoendstorage1.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "endtoenddatafactory53_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorageLS_accountKey": {
						"value": "[parameters('AzureDataLakeStorageLS_accountKey')]"
					},
					"AzureSqlDatabase_connectionString": {
						"value": "[parameters('AzureSqlDatabase_connectionString')]"
					},
					"AzureSqldbls_connectionString": {
						"value": "[parameters('AzureSqldbls_connectionString')]"
					},
					"endtoendls1_accountKey": {
						"value": "[parameters('endtoendls1_accountKey')]"
					},
					"AzureDataLakeStorageLS_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorageLS_properties_typeProperties_url')]"
					},
					"endtoendls1_properties_typeProperties_url": {
						"value": "[parameters('endtoendls1_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}